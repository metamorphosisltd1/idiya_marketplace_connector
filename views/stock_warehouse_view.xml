<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_warehouse_inherit_marketplace" model="ir.ui.view">
        <field name="name">stock.warehouse.form.view.inherit.marketplace</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Marketplace Config" name="marketplace_config_ids">
                    <field name="marketplace_config_ids" force_save="1"/>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
